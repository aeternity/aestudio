    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@uiowa/digit-only": "^1.3.0",
    "ansi_up": "^4.0.4",
    "crypto-browserify": "^3.12.0",
    "insert-line": "^1.1.0",
